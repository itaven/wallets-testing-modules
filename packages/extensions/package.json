    "rxjs": "^7.5.6",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/testing": "^8.2.5",
    "@types/unzipper": "^0.10.5"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false,
